<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Lead:</strong>
    <%= article.lead %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= article.content %>
  </p>

  <p>
    <strong>Published at:</strong>
    <%= article.published_at %>
  </p>

  <p>
    <strong>Slug:</strong>
    <%= article.slug %>
  </p>

</div>
